    <script type="text/javascript">
        /**
            Listen on QUANTITY dropdown menu change event, recalculate the local price and (shipping fee + ifd),
            then present it on top of buy box.
         */
        P.when("A", "ready").execute(function(A) {
            var quantityNode = A.$("#quantity");

            //define quantity dropdown box event handler
            var updateIFDInfo = function(event) {
                var quantity = quantityNode.val(),
                    asin = A.$("#ASIN").val(),
                    merchantId = A.$("#merchantID").val(),
                    url = "/gp/global-store/ajax/update-ifd.html?asin=" + asin + "&quantity=" + quantity + "&merchantId=" + merchantId;

                var shippingStringNode = A.$(event.data.shippingString),
                    shippingFeeSavedStringNode = A.$(event.data.shippingFeeSavedString),
                    priceNode = A.$(event.data.price);

                A.$.ajax({
                    url: url,
                    type: "GET",
                    dataType: "json",
                }).done(function (json) {
                    //change nothing if either ifd nor shippingFee is missing
                    if (json.ifd == null || json.shippingFee == null) {
                        P.log("Error: ifd or shipping fee is null" + JSON.stringify(json));
                        return ;
                    }

                    var price = json.formattedPrice,
                        fee = json.formattedTotalFee,
                        shippingFeeSaved = json.shippingFeeSaved,
                        isFreeShipping = json.agsPrimeFreeShippingEligible;

                    if (isFreeShipping) {
                        shippingFeeSavedStringNode.html("+ " + shippingFeeSaved + " " + json.buyboxShippingFeeString);
                    } else {
                        shippingFeeSavedStringNode.empty();
                    }
                    shippingStringNode.html("&nbsp;+ " + fee + " " + json.buyboxShippingString);
                    priceNode.html(price);
                    shippingStringNode.data("ags.ifd",json.ifd);
                    shippingStringNode.data("ags.shippingFee",json.shippingFee);
                }).fail(function (xhr, status, errThrown) {
                    P.log("Error: " + errThrown + ", Status: " + status);
                });
            }

            //register event
            if (quantityNode) {
                //for 3 column case
                quantityNode.change({
                    price: '#ags_local_price',
                    shippingString: '#ags_shipping_import_fee',
                    shippingFeeSavedString: '#ags_shipping_fee_saved'
                },updateIFDInfo);

                //for 2 column case
                /*
                quantityNode.change({
                    price: '#priceblock_ourprice',
                    shippingString: '#ourprice_shippingmessage .a-color-secondary'
                },updateIFDInfo);
                */
            }
        });
    </script>


    

    
    














    
    
    







